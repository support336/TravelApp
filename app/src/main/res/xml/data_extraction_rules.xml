<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="google_signin_prefs.xml"/>
        <exclude domain="database" path="travel_database"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for user data -->
        <include domain="database" path="travel_database"/>
    </device-transfer>
</data-extraction-rules>
